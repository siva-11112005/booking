{
  "name": "eswari-physiotherapy",
  "version": "1.0.0",
  "description": "Physiotherapy appointment booking system",
  "main": "backend/server.js",
  "scripts": {
  "start": "node backend/server.js",
  "build": "npm run clean-install && npm run install-frontend && npm run build-frontend",
  "clean-install": "cd backend && rm -rf node_modules package-lock.json && npm install",
  "install-frontend": "cd frontend && npm install",
  "build-frontend": "cd frontend && npm run build",
  "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "twilio": "^4.23.0",
    "i18next": "^23.7.6",
    "react-i18next": "^14.0.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "workbox-webpack-plugin": "^7.0.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "keywords": ["physiotherapy", "appointment", "booking"],
  "author": "Eswari",
  "license": "MIT"
}
